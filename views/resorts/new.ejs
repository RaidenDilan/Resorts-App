<section>
  <h1>New Resort</h1>
  <hr>
  <div class="container">
    <form method="POST" action="/resorts" enctype="multipart/form-data">
      <div>
        <label for="name">Name</label>
        <input type="text" name="name" id="name" class="form-control">
      </div>
      <div>
        <label for="website">Website</label>
        <input type="text" name="website" id="website" class="form-control">
      </div>
      <div>
        <label for="country">Country</label>
        <input type="text" name="country" id="country" class="form-control">
      </div>
      <div>
        <label for="city">City</label>
        <input type="text" name="city" id="city" class="form-control">
      </div>
      <div>
        <label for="lifts">Lifts</label>
        <input type="text" name="lifts" id="lifts" class="form-control">
      </div>
      <div>
        <label for="rating">Rating</label>
        <input type="text" name="rating" id="rating" class="form-control">
      </div>
      <div>
        <label for="address">Address</label>
        <input type="text" name="address" id="address" class="autocomplete form-control">
        <input type="hidden" name="lat">
        <input type="hidden" name="lng">
      </div>
      <div>
        <label for="image">Image</label>
        <input type="file" name="image" id="image">
      </div>
      <button class="deletebuttons submit">Add Resort</button>
    </form>
  </div>
</section>
